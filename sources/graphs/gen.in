set terminal epslatex size 6,3.5
set output "graphs/gen.tex"

set grid
set key right top
set title ""
set xlabel "log$_{10}\\Big(\\big|$error$\\big|\\Big)$"
set ylabel "Frequency"
set samples 10000

plot [-15:10][:]'sources/graphs/gen.dat' using 1:2 title 'Solver generated by the systematical generator' smooth bezier lt 1 lc rgb "blue" lw 3, \
     'sources/graphs/gen.dat' using 1:3 title 'Solver using the $F_4$ strategy' smooth bezier lt 1 lc rgb "red" lw 3, \

